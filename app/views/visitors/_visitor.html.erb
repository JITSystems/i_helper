<div class="message" id="<%= visitor.id %>">
	<span><%= visitor.created_at.strftime("%H:%M") %></span>
	<span><%= visitor.fio %></span>
	<span><%= visitor.time %></span>
	<span><%= visitor.theme %></span>
	<span class="msg_del_ico" style="float:right; margin-left:5px;"><%=link_to 'Destroy', visitor, method: :delete, remote: true %></span>
	<span class="msg_del_ico" style="float:right"><%=link_to 'Edit', visitor, method: :put, remote: true %></span>
</div>